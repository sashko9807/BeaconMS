{
  "name": "beaconms",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "android:bundle": "mkdirp android/app/src/main/assets && npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "android:buildDebug": "yarn android:bundle && cd android && gradlew --stop && gradlew assembleDebug",
    "android:installDebug": "cd android && gradlew installDebug",
    "android:buildRelease": "yarn android:bundle && cd android && gradlew --stop && gradlew assembleRelease",
    "android:installRelease": "cd android && gradlew installRelease",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "dependencies": {
    "@expo/webpack-config": "~0.16.2",
    "@react-native-community/checkbox": "^0.5.14",
    "@react-native-picker/picker": "^2.4.1",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.8.2",
    "@types/react-native": "^0.67.9",
    "axios": "^0.27.2",
    "expo": "~45.0.0",
    "expo-app-loading": "~2.0.0",
    "expo-image-manipulator": "^10.3.1",
    "expo-image-picker": "~13.1.1",
    "expo-navigation-bar": "~1.2.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "glob": "^7.2.0",
    "queue-microtask": "^1.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.34.1",
    "react-native": "0.68.5",
    "react-native-beacons-manager": "github:sashko9807/react-native-beacons-manager",
    "react-native-bluetooth-state-manager": "github:sashko9807/react-native-bluetooth-state-manager",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-gradle-plugin": "^0.0.7",
    "react-native-image-picker": "^4.8.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-location-enabler": "github:sashko9807/react-native-location-enabler.git",
    "react-native-navigation-bar-color": "^2.0.1",
    "react-native-reanimated": "^2.12.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "3.14.1",
    "react-native-svg": "^12.3.0",
    "react-native-web": "0.17.7",
    "react-native-webview": "^11.23.0",
    "react-redux": "^8.0.2",
    "webpack-dev-server": "~3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "mkdirp": "^3.0.1",
    "react-native-dotenv": "^3.4.8",
    "react-native-svg-transformer": "^1.0.0",
    "yarn-upgrade-all": "^0.7.1"
  },
  "private": true
}
